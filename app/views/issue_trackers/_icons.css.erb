/* Issue Tracker inactive, select, create and goto icons */
<% trackers = IssueTracker.subclasses.map{|t| t::Label } << 'none' %>
<% trackers.each do |tracker| %>
div.issue_tracker.nested label.<%= tracker %> { background: url(/images/<%= tracker %>_inactive.png) no-repeat; }
div.issue_tracker.nested label.r_on.<%= tracker %>, #action-bar a.<%= tracker %>_create { background: url(/images/<%= tracker %>_create.png) no-repeat; }
#action-bar a.<%= tracker %>_goto { background: transparent url(/images/<%= tracker %>_goto.png) 6px 5px no-repeat; }
<% end %>

