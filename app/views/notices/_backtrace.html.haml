.window
  %table.backtrace
    %tr
      %th
        %ul.line-numbers
          - lines.each do |line|
            %li= line['number']
      %td
        %ul.lines
          - lines.each do |line|
            - in_app = line['file'].gsub!('[PROJECT_ROOT]','') && !line['file'].match(/^\/vendor\//)
            %li{:class => (in_app ? 'in-app' : nil)}
              = line['file']
              &rarr;
              %strong= line['method']