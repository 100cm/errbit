.window
  %table.backtrace
    %tr
      %th
        %ul.line-numbers
          - lines.each do |line|
            %li
              - if line['number'].present?
                = (@app.github_url? && Notice.in_app_backtrace_line?(line)) ? link_to_github(@app, line, line['number']) : line['number']
              - else
                &nbsp;
      %td
        %ul.lines
          - lines.each do |line|
            - in_app = line['file'].to_s.gsub!('[PROJECT_ROOT]','') && !line['file'].to_s.match(/^\/vendor\//)
            %li{:class => (in_app ? 'in-app' : nil)}
              = line['file']
              &rarr;
              %strong= line['method']

